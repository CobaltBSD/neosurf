    <member>"em"</member>
    <member>"strong"</member>
    <member>"code"</member>
    <member>"sup"</member>
    <member>"var"</member>
    <member>"acronym"</member>
</var>
<var name="svgExpectedNames" type="List">
    <member>"svg"</member>
    <member>"rect"</member>
    <member>"script"</member>
    <member>"head"</member>
    <member>"meta"</member>
    <member>"title"</member>
